{
    "name": "deckmeout",
    "version": "1.0.0",
    "description": "A flashcard MERN stack app",
    "main": "server/server.js",
    "scripts": {
        "build": "cd client && npm run build",
        "install-client": "cd client && npm install",
        "install-server": "cd server && npm install",
        "heroku-postbuild": "npm run install-client && npm run build",
        "start": "nodemon server/server.js",
        "develop": "concurrently --kill-others-on-fail \"npm start\"  \"npm run start --prefix client\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bennygo3/deckmeout"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/clabel95/Decked-Out/issues"
    },
    "homepage": "https://clabel95.github.io/Decked-Out",
    "type": "module",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "concurrently": "^7.0.0",
        "dotenv": "^16.4.5",
        "graphql": "^16.3.0",
        "jwt-decode": "^3.1.2"
    }
}
